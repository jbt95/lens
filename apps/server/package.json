{
	"name": "@lens/server",
	"version": "0.0.0",
	"scripts": {
		"build": "tsc",
		"deploy": "cdk bootstrap && cdk deploy",
		"type:check": "tsc --noEmit",
		"test:unit": "mocha --require ts-node/register --require source-map-support/register --require tsconfig-paths/register --recursive --spec './src/__tests__/unit/**/*.test.ts'",
		"test:integration": "mocha --require ts-node/register --require source-map-support/register --recursive --spec './src/__tests__/integration/**/*.test.ts'",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint . --ext .ts --fix"
	},
	"devDependencies": {
		"@lens/eslint-config": "workspace:^0.0.0",
		"@lens/internal": "workspace:^0.0.0",
		"@lens/ts-config": "workspace:^0.0.0",
		"@types/aws-lambda": "^8.10.119",
		"@types/mocha": "^10.0.1",
		"@types/node": "^20.1.7",
		"aws-cdk": "2.87.0",
		"aws-cdk-lib": "2.87.0",
		"aws-sdk": "^2.1339.0",
		"constructs": "^10.0.0",
		"mocha": "^10.2.0",
		"source-map-support": "^0.5.21",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "~5.1.3"
	},
	"dependencies": {
		"inversify": "^6.0.1",
		"reflect-metadata": "^0.1.13",
		"rxjs": "^7.8.1",
		"zod": "^3.21.4"
	}
}
