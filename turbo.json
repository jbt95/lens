{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "type:check": {},
		"format": {},
		"deploy": {
			"dependsOn": [
				"test:unit",
				"^deploy"
			]
		},
		"lint": {
			"dependsOn": [
				"type:check",
				"^lint"
			]
		},
		"test:unit": {},
		"test:integration": {
			"dependsOn": [
				"deploy",
				"^test:integration"
			],
			"env": [
				"NODE_ENV=test",
				"STAGE=test"
			]
		}
  }
}
