{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": [
					"build",
					"lint",
					"type-check",
					"test:unit"
				],
				"parallel": 10
			}
		}
	},
	"targetDefaults": {
		"type:check": {},
		"deploy": {
			"dependsOn": [
				"^test:unit"
			]
		},
		"lint": {
			"dependsOn": [
				"^type:check",
				"^lint"
			]
		},
		"test:unit": {},
		"test:integration": {
			"dependsOn": [
				"^deploy"
			]
		}
	},
	"defaultBase": "main"
}
